	      <Order>2</Order>
	      <Command><![CDATA[UPDATE Processing SET [HomeAddressStreetNum] = Substring([HomeAddressStreetName], patindex('%[0-9]%', [HomeAddressStreetName]), 1+patindex('%[0-9][^0-9]%', [HomeAddressStreetName]+'x')-patindex('%[0-9]%', [HomeAddressStreetName])) where [HomeAddressStreetNum] is null ]]></Command>
